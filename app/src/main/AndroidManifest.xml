<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nechay.visualiser">

    <application
        android:allowBackup="true"
        android:icon="@drawable/graph"
        android:label="@string/app_name"
        android:roundIcon="@drawable/graph"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:windowSoftInputMode="stateVisible">
        <activity android:name=".DescriptionActivity"></activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".VisualaserActivity" />
        <activity android:name=".NewGraphActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>